{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "lint": "eslint src/",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ],
    "**/*.{jsx,js}": [
      "eslint src/"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.5",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.0.13",
    "antd": "^4.1.0",
    "axios": "^0.19.2",
    "basscss": "^8.1.0",
    "classnames": "^2.2.6",
    "dva-logger": "^1.0.0",
    "js-cookie": "^2.2.1",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-numpad": "^5.0.4",
    "react-simple-line-icons": "^1.0.8",
    "styled-components": "^5.0.1",
    "umi": "^3.0.13",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.3"
  }
}
